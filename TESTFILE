#include<iostream>
using namespace std;


class gabby
{
      public:
             int x, y, sum;
             int test()
             {
                 sum=1+1;
                 cout<<sum;
                 return 0;
             }

};

int main()
{
    gabby gab;
    cout<<"Hello world\n\n";
    gab.test();
    cout<<endl;
system("pause");
return 0;
}



